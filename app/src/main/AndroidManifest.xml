<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tienthanh.foodstore">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FoodDetailActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EditFoodActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".UserDetailActivity" />
        <activity android:name=".EditUserActivity" />
        <activity android:name=".VendorDetailActivity" />
        <activity android:name=".EditVendorActivity" />
        <activity android:name=".OrderDetailActivity" />
        <activity android:name=".CartDetailActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MakeOrderActivity" />
        <activity android:name=".BillDetailActivity" />
        <activity android:name=".AboutFoodStoreActivity"></activity>
    </application>

</manifest>